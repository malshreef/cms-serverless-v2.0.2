# S7ABT Admin Panel Environment Variables
# Copy this file to .env and fill in your values
# NEVER commit .env to version control

# AWS Cognito Configuration
VITE_COGNITO_USER_POOL_ID=<your-cognito-user-pool-id>
VITE_COGNITO_CLIENT_ID=<your-cognito-client-id>
VITE_AWS_REGION=<your-aws-region>

# API Configuration (from Admin API SAM deploy output)
VITE_API_ENDPOINT=https://<your-admin-api-id>.execute-api.<your-region>.amazonaws.com/<stage>
VITE_API_BASE_URL=https://<your-admin-api-id>.execute-api.<your-region>.amazonaws.com/<stage>

# S3 Media Storage
VITE_S3_BASE_URL=https://<your-bucket>.s3.<your-region>.amazonaws.com

# Push Notifications (VAPID Public Key)
VITE_VAPID_PUBLIC_KEY=<your-vapid-public-key>
